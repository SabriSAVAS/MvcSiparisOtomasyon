@model SiparisEnt.Dto.CatalogVM.CustomerAuthorizedPopupModel

<table class="table table-bordered table-condensed">
    <thead>
    <tr>
        <th>Yetkili Ad</th>
        <th>Yetkili Soyad</th>
        <th>İşlemler</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.CustomerAuthorizedList)
    {
        <tr>
            <td>
                <label id="customerauthorizedFirstName_@item.Id" data-id="@item.FirstName"> @item.FirstName</label>
            </td>
            <th>
                <label id="customerauthorizedLastName_@item.Id" data-id="@item.LastName"> @item.LastName</label>
            </th>
            <td class="center">

                <a href="javascript:customerauthorizedselect(@item.Id)" class="btn btn-sm btn-default">
                    <i class="glyphicon glyphicon-ok"></i>
                </a>
            </td>
        </tr>
        }
    </tbody>
</table>
